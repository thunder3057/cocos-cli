        "reflect-metadata": "^0.2.2",
        "replace-in-file": "^8.3.0",
        "rimraf": "^2.7.1",
        "rollup-plugin-terser": "^7.0.2",
        "rotating-file-stream": "^3.2.7",
        "semver": "^7.7.2",
        "sharp": "^0.32.6",
        "socket.io": "^4.8.1",
        "socket.io-client": "^4.8.1",
        "strip-ansi": "^6.0.1",
        "tga-js": "^1.1.1",
        "tmp": "^0.0.33",
        "urijs": "^1.19.11",
        "uuid": "^8.3.2",
        "xhr2": "^0.2.1",
        "xmldom": "^0.1.31",
        "xxhashjs": "^0.2.2",
        "zod": "^3.25.76",
        "zod-to-ts": "1.1.4"
    }
}
